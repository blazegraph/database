[query2]
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ub: <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#>
SELECT ?x ?y ?z
WHERE{
	?x a ub:GraduateStudent .
	?y a ub:University .
	?z a ub:Department .
	?x ub:memberOf ?z .
	?z ub:subOrganizationOf ?y .
	?x ub:undergraduateDegreeFrom ?y
}
