<?xml version="1.0" encoding="UTF-8"?>
<!-- sample data with provenance information. -->
<!-- $Id: provenance.rdf 2265 2009-10-26 12:51:06Z thompsonbry $ -->
<!DOCTYPE data [
<!ENTITY bigdata "http://www.bigdata.com/rdf#">
]>

<rdf:RDF
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:bigdata="http://www.bigdata.com/rdf#"
    xml:base="&bigdata;"
>

<rdf:Description rdf:about="#Mike" >
    <rdfs:label>Mike</rdfs:label>
    <bigdata:loves rdf:resource="#bigdata" />
</rdf:Description>
  
<rdf:Description rdf:about="#Bryan" >
    <rdfs:label>Bryan</rdfs:label>
    <bigdata:loves rdf:resource="#bigdata" />
</rdf:Description>
  
</rdf:RDF>

